<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wordhunter.client.ui.StartPageController">
   <children>
      <Button layoutX="250.0" layoutY="250.0" mnemonicParsing="false" onAction="#createButtonClicked" prefHeight="50.0" prefWidth="300.0" text="Create Server">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="250.0" layoutY="500.0" mnemonicParsing="false" onAction="#joinButtonClicked" prefHeight="50.0" prefWidth="300.0" text="Join Server">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <TextField fx:id="textField" layoutX="250.0" layoutY="450.0" prefHeight="25.0" prefWidth="300.0" />
      <Label fx:id="label" layoutX="275.0" layoutY="410.0" text="Enter Server's IP address.">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="125.0" layoutY="50.0" text="Welcome to Word Hunter!!!!">
         <font>
            <Font size="48.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
